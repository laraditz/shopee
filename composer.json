{
  "name": "laraditz/shopee",
  "description": "A simple laravel package for Shopee",
  "keywords": [
    "laraditz",
    "shopee"
  ],
  "homepage": "https://github.com/laraditz/shopee",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Raditz Farhan",
      "email": "raditzfarhan@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Laraditz\\Shopee\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laraditz\\Shopee\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laraditz\\Shopee\\ShopeeServiceProvider"
      ],
      "aliases": {
        "Shopee": "Laraditz\\Shopee\\Facades\\Shopee"
      }
    }
  }
}
